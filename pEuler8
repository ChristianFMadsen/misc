import numpy as np

print("Problem 8")

numberStr = ""
numberList = list(numberStr)
numberList = list(map(int, numberList))
numberArr = np.array(numberList,dtype='int64')


biggestNumber = 0


for i in range(np.size(numberArr)-12):
    currentProduct = numberArr[i]*numberArr[i+1]*numberArr[i+2]*numberArr[i+3]*numberArr[i+4]*numberArr[i+5]*numberArr[i+6]*numberArr[i+7]*numberArr[i+8]*numberArr[i+9]*numberArr[i+10]*numberArr[i+11]*numberArr[i+12]
    if currentProduct > biggestNumber:
        biggestNumber = currentProduct


print(biggestNumber)


